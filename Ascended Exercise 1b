public class Matrix {
    public static void main (String args[]){

        int a[] = {1, 8};
        int b[] = {2, 9};
        int product = 0;

        for(int c = 0; c < a.length; c++){
            product += a[c] * b[c];
        }
        System.out.println("The dot product is: "+product);
    }
}
